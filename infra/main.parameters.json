{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "environmentName": {
            "value": "${AZURE_ENV_NAME}"
        },
        "location": {
            "value": "${AZURE_LOCATION}"
        },
        "fastApiEnv": {
            "value": "production"
        },
        "logLevel": {
            "value": "INFO"
        },
        "corsOrigins": {
            "value": "*"
        },
        "apiVersion": {
            "value": "v2.0"
        },
        "streamlitServerPort": {
            "value": "8501"
        },
        "streamlitServerAddress": {
            "value": "0.0.0.0"
        },
        "streamlitThemeBase": {
            "value": "light"
        }
    }
}